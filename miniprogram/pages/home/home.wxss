page {
  background-color: #F2F2F7;
  height: 100%;
  overflow: hidden;
  font-family: -apple-system, "SF Pro Rounded", sans-serif;
}

/* ================= è‡ªåŠ¨æ¶ˆå¤±æç¤ºï¼ˆæ— æŒ‰é’®ï¼Œ3ç§’åè‡ªåŠ¨æ¶ˆå¤±ï¼Œå¸¦æ”¶ç¼©é€€å‡ºåŠ¨ç”»ï¼‰ ================= */
.auto-toast-mask {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0);
  z-index: 99999;
  display: flex;
  align-items: center;
  justify-content: center;
  pointer-events: none;
  animation: autoToastMaskFadeIn 0.25s ease-out forwards;
}
.auto-toast-mask.closing {
  animation: autoToastMaskFadeOut 0.4s cubic-bezier(0.4, 0, 0.2, 1) forwards;
}
@keyframes autoToastMaskFadeIn {
  from { opacity: 0; }
  to { opacity: 1; }
}
@keyframes autoToastMaskFadeOut {
  from { opacity: 1; }
  to { opacity: 0; }
}

.auto-toast-box {
  background: #fff; /* ğŸ”´ æ”¹ä¸ºç™½è‰²èƒŒæ™¯ */
  backdrop-filter: blur(20rpx);
  border-radius: 24rpx;
  padding: 24rpx 40rpx;
  max-width: 600rpx;
  text-align: center;
  animation: toastFadeIn 0.35s cubic-bezier(0.34, 1.56, 0.64, 1) forwards;
  pointer-events: auto;
  box-shadow: 0 8rpx 30rpx rgba(0,0,0,0.15); /* ğŸ”´ æ·»åŠ é˜´å½± */
}
.auto-toast-mask.closing .auto-toast-box {
  animation: autoToastShrinkOut 0.4s cubic-bezier(0.4, 0, 0.2, 1) forwards;
}
@keyframes toastFadeIn {
  from {
    opacity: 0;
    transform: translateY(-20rpx) scale(0.92);
  }
  to {
    opacity: 1;
    transform: translateY(0) scale(1);
  }
}
@keyframes autoToastShrinkOut {
  0% {
    transform: scale(1) translateY(0);
    opacity: 1;
    filter: blur(0);
  }
  100% {
    transform: scale(0.6) translateY(-60rpx);
    opacity: 0;
    filter: blur(12rpx);
  }
}

.at-title {
  font-size: 28rpx;
  font-weight: 600;
  color: #000; /* ğŸ”´ æ”¹ä¸ºé»‘è‰² */
  margin-bottom: 8rpx;
}

.at-content {
  font-size: 26rpx;
  color: #000; /* ğŸ”´ æ”¹ä¸ºé»‘è‰² */
  line-height: 1.5;
}

/* ================= è‡ªå®šä¹‰å¯¹è¯æ¡†ï¼ˆå¸¦å¼¹å‡º+æ”¶ç¼©é€€å‡ºåŠ¨ç”»ï¼‰ ================= */
.custom-dialog-mask {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.6);
  backdrop-filter: blur(5px);
  z-index: 99999;
  display: flex;
  align-items: center;
  justify-content: center;
  animation: dialogMaskFadeIn 0.25s ease-out forwards;
}
.custom-dialog-mask.closing {
  animation: dialogMaskFadeOut 0.4s cubic-bezier(0.4, 0, 0.2, 1) forwards;
}
@keyframes dialogMaskFadeIn {
  from { opacity: 0; }
  to { opacity: 1; }
}
@keyframes dialogMaskFadeOut {
  from { opacity: 1; }
  to { opacity: 0; }
}

.custom-dialog-box {
  width: 600rpx;
  background: #fff;
  border-radius: 40rpx;
  padding: 50rpx 40rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  text-align: center;
  box-shadow: 0 20rpx 60rpx rgba(0,0,0,0.2);
  animation: dialogBoxPopIn 0.35s cubic-bezier(0.34, 1.56, 0.64, 1) forwards;
}
.custom-dialog-mask.closing .custom-dialog-box {
  animation: dialogBoxShrinkOut 0.4s cubic-bezier(0.4, 0, 0.2, 1) forwards;
}
@keyframes dialogBoxPopIn {
  from { opacity: 0; transform: scale(0.88); }
  to { opacity: 1; transform: scale(1); }
}
@keyframes dialogBoxShrinkOut {
  0% {
    transform: scale(1) translateY(0);
    opacity: 1;
    filter: blur(0);
  }
  100% {
    transform: scale(0.6) translateY(-60rpx);
    opacity: 0;
    filter: blur(12rpx);
  }
}

.cd-title { font-size: 34rpx; font-weight: 800; margin-bottom: 20rpx; color: #000; }
.cd-content { font-size: 28rpx; color: #000; margin-bottom: 50rpx; line-height: 1.5; }

.cd-btn-row { width: 100%; display: flex; gap: 30rpx; }
.cd-btn { flex: 1; height: 88rpx; line-height: 88rpx; border-radius: 44rpx; font-size: 30rpx; font-weight: 600; }
.cd-btn.confirm { background: #000; color: #fff; }
.cd-btn:active { opacity: 0.8; transform: scale(0.98); }

.container {
  width: 100%;
  height: 100%;
  position: relative;
  overflow: hidden;
}

