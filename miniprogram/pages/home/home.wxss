page {
  background-color: #F2F2F7;
  height: 100%;
  overflow: hidden;
  font-family: -apple-system, "SF Pro Rounded", sans-serif;
}

.container {
  width: 100%;
  height: 100%;
  position: relative;
}

/* 左侧装饰文字 */
.bg-deco {
  position: absolute;
  left: 40rpx;
  top: 50%;
  transform: translateY(-50%);
  font-size: 100rpx;
  font-weight: 900;
  color: rgba(0,0,0,0.06);
  writing-mode: vertical-rl;
  text-orientation: upright;
  letter-spacing: 10rpx;
  pointer-events: none; /* 关键：让点击穿透，背景点击才能生效 */
}

/* 轮盘容器 */
.wheel-stage {
  position: absolute; right: 0; top: 0; bottom: 0; width: 100%;
  z-index: 10;
  /* 遮罩：让上下边缘渐隐 */
  mask-image: linear-gradient(to bottom, transparent 5%, black 25%, black 75%, transparent 95%);
  -webkit-mask-image: linear-gradient(to bottom, transparent 5%, black 25%, black 75%, transparent 95%);
  transition: opacity 0.2s;
}
.wheel-stage.hidden { opacity: 0; pointer-events: none; }

/* 列表卡片 */
.card {
  position: absolute; right: -80rpx; top: 50%;
  width: 640rpx; height: 260rpx; margin-top: -130rpx;
  background: #FFFFFF; border-radius: 48rpx;
  box-shadow: 0 8rpx 24rpx rgba(0,0,0,0.05);
  display: flex; align-items: center; justify-content: space-between;
  padding: 0 56rpx;
  box-sizing: border-box;
  transform-origin: 1400rpx 50%; /* 旋转轴心 */
  will-change: transform;
  overflow: hidden;
  transition: box-shadow 0.3s;
}

.c-left { display: flex; flex-direction: column; gap: 12rpx; width: 60%; }
.c-name { font-size: 38rpx; font-weight: 800; color: #999; transition: 0.3s; white-space: nowrap; overflow: hidden; text-overflow: ellipsis;}
.c-services-row { display: flex; gap: 12rpx; height: 40rpx; align-items: center;}
.cs-icon { font-size: 28rpx; opacity: 0.5; filter: grayscale(1); }
.cs-icon.has { opacity: 1; filter: grayscale(0); }

.c-right { text-align: right; width: 35%; }
.c-dist { font-size: 52rpx; font-weight: 800; font-family: monospace; color: #ccc; transition: color 0.3s;}
.c-unit { font-size: 24rpx; font-weight: 600; color: #ccc; margin-left: 4rpx; }

/* 选中态 */
.card.active { background: #fff; z-index: 100; box-shadow: 0 20rpx 60rpx rgba(0,0,0,0.08); }
.card.active .c-name, .card.active .c-dist, .card.active .c-unit { color: #000; }

/* --- 替身层 --- */
.phantom {
  position: fixed; background: #fff; border-radius: 48rpx;
  box-shadow: 0 8rpx 24rpx rgba(0,0,0,0.05); z-index: 999;
  transform-origin: top left; overflow: hidden;
  display: flex; flex-direction: column;
  transition: all 0.5s cubic-bezier(0.19, 1, 0.22, 1);
}
.phantom.expanded { 
  box-shadow: 0 80rpx 200rpx rgba(0,0,0,0.25); 
  border-radius: 64rpx; 
}

/* 1. 列表内容克隆体 (解决文字恢复慢的核心) */
.p-list-content {
  position: absolute; top: 0; left: 0; width: 100%; height: 260rpx;
  display: flex; align-items: center; justify-content: space-between;
  padding: 0 56rpx; box-sizing: border-box;
  opacity: 1; 
  /* 关键：transition时间设为0，移除 expanded 类时瞬间显示 */
  transition: opacity 0s; 
}
.phantom.expanded .p-list-content { 
  opacity: 0; 
  pointer-events: none; 
  transition: opacity 0.2s; /* 打开时可以有淡出动画 */
}

/* 2. 详情图 */
.p-image-box {
  width: 100%; height: 0; overflow: hidden; position: relative;
  transition: height 0.5s cubic-bezier(0.19, 1, 0.22, 1);
}
.phantom.expanded .p-image-box { height: 32%; }
.p-img { width: 100%; height: 100%; object-fit: cover; transform: scale(1.1); opacity: 0; transition: 0.4s; }
.phantom.expanded .p-img { transform: scale(1); opacity: 1; transition-delay: 0.1s; }

/* 按钮组 */
.p-btn-group {
  position: absolute; top: 40rpx; right: 40rpx; display: flex; gap: 20rpx; z-index: 10;
  opacity: 0; transition: 0.2s; pointer-events: none;
}
.phantom.expanded .p-btn-group { opacity: 1; pointer-events: auto; transition-delay: 0.2s; }
.circle-btn {
  width: 72rpx; height: 72rpx; 
  background: rgba(255,255,255,0.2); backdrop-filter: blur(20px);
  border: 1px solid rgba(255,255,255,0.3); border-radius: 50%; color: #fff;
  display: flex; align-items: center; justify-content: center; font-weight: bold; font-size: 28rpx;
}
.edit-btn { background: #000; border: none; }

/* 3. 详情内容 */
.p-content {
  flex: 1; opacity: 0; transform: translateY(40rpx);
  transition: opacity 0.1s, transform 0.1s;
  overflow-y: auto; background: #fff; position: relative;
  margin-top: -40rpx; border-top-left-radius: 48rpx; border-top-right-radius: 48rpx; 
  padding: 60rpx 48rpx; box-sizing: border-box;
}
.phantom.expanded .p-content {
  opacity: 1; transform: translateY(0);
  transition: opacity 0.4s 0.2s, transform 0.4s 0.2s;
}

/* 详情元素 */
.title-row { display: flex; justify-content: space-between; align-items: flex-start; margin-bottom: 16rpx; }
.d-title { font-size: 52rpx; font-weight: 900; line-height: 1.1; color: #000; width: 70%; }
.d-dist-badge { background: #F5F5F7; padding: 12rpx 24rpx; border-radius: 24rpx; font-family: monospace; font-size: 32rpx; font-weight: 800; color: #000;}

.d-sub-row { display: flex; align-items: center; gap: 20rpx; margin-bottom: 48rpx; }
.status-pill { display: flex; align-items: center; gap: 12rpx; font-size: 24rpx; font-weight: 700; color: #333; background: rgba(0,200,83, 0.1); padding: 8rpx 20rpx; border-radius: 16rpx;}
.status-dot { width: 12rpx; height: 12rpx; border-radius: 50%; background: #00C853; }
.sub-text { font-size: 26rpx; color: #999; font-weight: 500; }

.tag-cloud { display: flex; flex-wrap: wrap; gap: 16rpx; margin-bottom: 60rpx; }
.srv-tag { padding: 16rpx 28rpx; border: 1px solid #E5E5EA; border-radius: 20rpx; font-size: 24rpx; font-weight: 600; color: #555; display: flex; align-items: center; gap: 12rpx;}

/* --- 3. 修改信息块：压缩内部间距 --- */
.info-block { 
  background: #F9F9F9; 
  border-radius: 32rpx; 
  padding: 32rpx; /* 减小内边距 */
  display: flex; 
  flex-direction: column; 
  /* 减小行与行之间的间距 */
  gap: 24rpx; 
}
.ib-row { display: flex; align-items: flex-start; gap: 32rpx; }
.ib-icon { width: 72rpx; height: 72rpx; background: #fff; border-radius: 20rpx; display: flex; align-items: center; justify-content: center; font-size: 36rpx; box-shadow: 0 4rpx 16rpx rgba(0,0,0,0.03);}
.ib-content { flex: 1; }
.ib-label { font-size: 22rpx; font-weight: 700; color: #AAA; letter-spacing: 1rpx; margin-bottom: 8rpx; display: block; }
.ib-val { font-size: 30rpx; font-weight: 600; color: #333; line-height: 1.4; }

