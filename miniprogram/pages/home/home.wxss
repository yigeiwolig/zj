.time-picker-row {
  display: flex;
  align-items: center;
  justify-content: center;
  gap: 20rpx;
  width: 100%;
}

.time-box {
  background: #F7F8FA;
  padding: 24rpx 40rpx;
  border-radius: 20rpx;
  font-size: 36rpx;
  font-weight: 800;
  color: #000;
  min-width: 160rpx;
  text-align: center;
